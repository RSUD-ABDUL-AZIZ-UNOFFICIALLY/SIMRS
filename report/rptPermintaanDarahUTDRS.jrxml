	<queryString>
		<![CDATA[SELECT
	permintaan_darah.noorder, 
	permintaan_darah.no_rawat, 
	DATE_FORMAT(permintaan_darah.tgl_permintaan,'%d-%m-%Y') AS tgl_permintaan, 
	permintaan_darah.jam_permintaan, 
	DATE_FORMAT(permintaan_darah.tgl_diperlukan,'%d-%m-%Y') AS tgl_diperlukan, 
	permintaan_darah.jam_diperlukan, 
	permintaan_darah.dokter_perujuk, 
	permintaan_darah.`status`, 
	permintaan_darah.diagnosa_klinis, 
	permintaan_darah.alasan_transfusi, 
	permintaan_darah.kadar_hb, 
	permintaan_darah.riwayat_transfusi, 
	permintaan_darah.jmlh_kehamilan, 
	permintaan_darah.pernah_abortus, 
	permintaan_darah.riwayat_hdn, 
	permintaan_darah.reaksi_transfusi, 
	permintaan_darah.ket_reaksi_transfusi, 
	permintaan_darah.periksa_uji_coombs, 
	permintaan_darah.ket_periksa_uji_coombs, 
	permintaan_darah.cek_gd, 
	permintaan_darah.ket_cek_gd, 
	permintaan_darah.kelas, 
	permintaan_darah.kamar, 
	permintaan_darah.nama_kamar, 
	permintaan_darah.darah_lengkap, 
	permintaan_darah.plasma, 
	permintaan_darah.plasma_konvalensen_covid, 
	permintaan_darah.packed_red_cell, 
	permintaan_darah.thrombocytr_concentrate, 
	permintaan_darah.lainnya, 
	dokter.nm_dokter, 
	pasien.no_rkm_medis, 
	pasien.nm_pasien, 
	pasien.jk, 
	pasien.tgl_lahir, 
	pasien.alamat, 
	penjab.png_jawab, 
	reg_periksa.umurdaftar, 
	reg_periksa.sttsumur
FROM
	permintaan_darah
	INNER JOIN
	dokter
	ON 
		permintaan_darah.dokter_perujuk = dokter.kd_dokter
	INNER JOIN
	reg_periksa
	ON 
		permintaan_darah.no_rawat = reg_periksa.no_rawat
	INNER JOIN
	pasien
	ON 
		reg_periksa.no_rkm_medis = pasien.no_rkm_medis
	INNER JOIN
	penjab
	ON 
		pasien.kd_pj = penjab.kd_pj AND
		reg_periksa.kd_pj = penjab.kd_pj
    where permintaan_darah.noorder= $P{noorder} 
    and permintaan_darah.no_rawat= $P{norawat}
    and permintaan_darah.tgl_permintaan= $P{tglpermintaan} 
    and permintaan_darah.jam_permintaan=$P{jampermintaan} 
    and permintaan_darah.dokter_perujuk=$P{dokter}]]>
	</queryString>
